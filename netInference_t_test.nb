         " ", "other", " ", "domains"}]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"weights", "=", "\[IndentingNewLine]", 
      RowBox[{"Table", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Drop", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"Table", "[", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"weightComputing", "[", 
             RowBox[{
              RowBox[{"zeroMeanSignals", "[", 
               RowBox[{"[", "j", "]"}], "]"}], ",", 
              RowBox[{"zeroMeanSignals", "[", 
               RowBox[{"[", "i", "]"}], "]"}], ",", 
              RowBox[{"net", "[", 
               RowBox[{"[", 
                RowBox[{"j", ",", "i"}], "]"}], "]"}]}], "]"}], ",", 
            "\[IndentingNewLine]", 
            RowBox[{"{", 
             RowBox[{"i", ",", "1", ",", "len"}], "}"}]}], "]"}], ",", 
          RowBox[{"{", "j", "}"}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", "1", ",", "len", ",", "1"}], "}"}]}], "]"}]}], ";",
      "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"I", " ", "compute", " ", "all", " ", "strengths"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"strengths", "=", 
      RowBox[{"Table", "[", 
